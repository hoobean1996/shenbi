{
  "name": "shenbi",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "神笔 (Shénbǐ) - A comprehensive child programming platform. Write code like drawing, and watch it come alive!",
  "dependencies": {
    "@lemonade/sdk": "file:./packages/lemonade-sdk",
    "@react-oauth/google": "^0.12.1",
    "html2canvas": "^1.4.1",
    "jspdf": "^4.0.0",
    "lucide-react": "^0.554.0",
    "react": "^18.3.1",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dnd-multi-backend": "^9.0.0",
    "react-dnd-touch-backend": "^16.0.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.9.6",
    "remark-gfm": "^4.0.1"
  },
  "scripts": {
    "start": "vite --host",
    "build": "npm run build:sdk && tsc -b && vite build",
